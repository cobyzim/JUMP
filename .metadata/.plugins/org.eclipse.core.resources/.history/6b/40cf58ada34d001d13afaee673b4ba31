package com.cognixia.jump.miniproject;

@SuppressWarnings("serial")
public class IncorrectBoundsException extends Exception{
	private final String errorMessage = "INVALID: Please enter a number between 1 and 5";
	
	public IncorrectBoundsException(String errorMessage) {
		
	}

	public String getErrorMessage() {
		return errorMessage;
	}
	
	
}
